{
    "name": "example-1",
    "version": "0.0.4",
    "private": true,
    "homepage": "https://andreneumann1990.github.io/ReactJS/example_1",
    "dependencies": {
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.89",
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "hammerjs": "^2.0.8",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.22.3",
        "react-scripts": "^5.0.1",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "deploy": "gh-pages -d build"
    },
    "eslintConfig": {
        "plugins": [
            "eslint-plugin-react"
        ],
        "extends": [
            "react-app"
        ],
        "rules": {
            "no-trailing-spaces": "error",
            "quotes": [
                "error",
                "single"
            ],
            "jsx-quotes": [
                "error",
                "prefer-double"
            ],
            "jsx-a11y/anchor-is-valid": "off",
            "space-infix-ops": [
                "error",
                {
                    "int32Hint": true
                }
            ],
            "new-cap": "off",
            "require-jsdoc": "off",
            "valid-jsdoc": "off",
            "no-invalid-this": "off",
            "no-undef": "error",
            "no-unused-vars": "warn",
            "no-var": "error",
            "prefer-const": "error",
            "prefer-rest-params": "error",
            "prefer-spread": "error",
            "prefer-template": "error",
            "arrow-spacing": "error",
            "generator-star-spacing": "error",
            "no-duplicate-imports": "error",
            "no-useless-rename": "error",
            "object-shorthand": "error",
            "prefer-arrow-callback": "error",
            "prefer-numeric-literals": "error",
            "prefer-object-spread": "error"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/hammerjs": "^2.0.45",
        "@types/tinycolor2": "^1.4.6",
        "gh-pages": "^6.1.1",
        "node-sass": "^9.0.0",
        "sass": "^1.72.0",
        "tinycolor2": "^1.6.0"
    }
}