{
    "name": "example_1_source_next",
    "version": "0.0.5",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "export": "next build && next export"
    },
    "dependencies": {
        "@use-gesture/react": "^10.3.1",
        "algoliasearch": "^4.22.1",
        "next": "^14.1.4",
        "react": "^18",
        "react-dom": "^18",
        "react-instantsearch": "^7.7.0",
        "react-router-dom": "^6.22.3",
        "sass": "^1.72.0",
        "tinycolor2": "^1.6.0"
    },
    "devDependencies": {
        "@babel/core": "^7.24.3",
        "@types/hammerjs": "^2.0.45",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "@types/tinycolor2": "^1.4.6",
        "autoprefixer": "^10.0.1",
        "babel-loader": "^9.1.3",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.1.4",
        "imports-loader": "^5.0.0",
        "postcss": "^8",
        "tailwindcss": "^3.3.0",
        "typescript": "^5",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4"
    },
    "eslintConfig": {
        "plugins": [
            "eslint-plugin-react"
        ],
        "extends": [
            "next",
            "next/core-web-vitals"
        ],
        "rules": {
            "no-trailing-spaces": "error",
            "quotes": [
                "error",
                "single"
            ],
            "jsx-quotes": [
                "error",
                "prefer-double"
            ],
            "jsx-a11y/anchor-is-valid": "off",
            "space-infix-ops": [
                "error",
                {
                    "int32Hint": true
                }
            ],
            "new-cap": "off",
            "require-jsdoc": "off",
            "valid-jsdoc": "off",
            "no-invalid-this": "off",
            "no-undef": "error",
            "no-unused-vars": "warn",
            "no-var": "error",
            "prefer-const": "warn",
            "prefer-rest-params": "error",
            "prefer-spread": "error",
            "prefer-template": "error",
            "arrow-spacing": "error",
            "generator-star-spacing": "error",
            "no-duplicate-imports": "error",
            "no-useless-rename": "error",
            "object-shorthand": "error",
            "prefer-arrow-callback": "error",
            "prefer-numeric-literals": "error",
            "prefer-object-spread": "error"
        }
    }
}
